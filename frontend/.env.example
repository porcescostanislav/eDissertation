# Frontend Environment Variables
# Copy this file to .env.local and fill in appropriate values

# API Configuration
VITE_API_URL=http://localhost:3000/api

# Application Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_DEBUG=false

# Authentication
VITE_JWT_TOKEN_KEY=auth_token

# Optional: Analytics
VITE_ANALYTICS_ID=

# Note: Variables must start with VITE_ to be accessible in the browser
# They are bundled at build time and become public
